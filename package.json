{
  "name": "sepa-js-xml",
  "keywords": [
    "sepa",
    "xml"
  ],
  "version": "2.0.4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/jerebtw/Sepa-JS-XML.git",
  "author": "Jerebtw <jeremialp41@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "size": "echo index && gzip-size lib/index.js && echo index.min && gzip-size lib/index.min.js",
    "build": "pnpm run ts && pnpm run minify",
    "minify": "terser lib/index.js -o lib/index.min.js -c --keep-fnames",
    "ts": "tsc && cd lib && copy index.d.ts index.min.d.ts",
    "docs": "typedoc src/index.ts --name \"Sepa JS XML\"",
    "deploy:docs": "pnpm run docs && gh-pages -d docs",
    "format:test": "prettier --check \"**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest && pnpm run format:test && pnpm run lint",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run test && pnpm run deploy:docs",
    "preversion": "pnpm run test",
    "version": "pnpm run format:write && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "gh-pages": "^4.0.0",
    "gzip-size-cli": "^5.1.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "terser": "^5.16.1",
    "ts-jest": "^29.0.4",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ibantools": "^4.2.2",
    "xml-js": "^1.6.11"
  }
}